package com.QA.BlackJackexp;

import static org.junit.Assert.*;

import org.junit.Test;

public class BlackJackTest {

	@Test
	public void testy() {
		BlackJack b = new BlackJack();
		assertEquals(0, b.play(-10, -20));
		@Test
		public void testy1() {
		assertEquals(0, b.play(-10, 10));
		@Test
		public void testy2() {
		assertEquals(0, b.play(-10, 30));
		@Test
		public void testy3() {
		assertEquals(0, b.play(-10, 50));
		@Test
		public void testy4() {
		assertEquals(0, b.play(20, 0));
		@Test
		public void testy5() {
		assertEquals(20, b.play(20, 12));
		@Test
		public void testy6() {
		assertEquals(20, b.play(20, 24));
		@Test
		public void testy7() {
		assertEquals(0, b.play(20, 33));
		@Test
		public void testy8() {
		assertEquals(0, b.play(22, -4));
		@Test
		public void testy9() {
		assertEquals(0, b.play(23, 22));
		@Test
		public void testy10() {
		assertEquals(0, b.play(23, 32));
		@Test
		public void testy11() {
		assertEquals(0, b.play(25, 4));
		@Test
		public void testy12() {
		assertEquals(0, b.play(35, -22));
		@Test
		public void testy13() {
		assertEquals(0, b.play(35, 2));
		@Test
		public void testy14() {
		assertEquals(0, b.play(32, 22));
		@Test
		public void testy15() {
		assertEquals(0, b.play(32, 35));
		@Test
		public void testy16() {
		assertEquals(0, b.play(32, 32));
		
	}
}
