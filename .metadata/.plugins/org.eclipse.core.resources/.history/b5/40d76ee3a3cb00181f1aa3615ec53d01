import java.awt.Button;
import java.awt.Label;
import java.awt.TextField;
import java.awt.event.ActionListener;

public class Operations implements ActionListener {
	
	TextField T1; 
	TextField T2; 
	TextField T3;
	
	Button B1;
	Button B2;
	Button B3;
	
	Label L1; 
	Label L2; 
	Label L3; 

	public Operations(TextField A, TextField B, TextField C, Button D, Button E, Button F, Label G, Label H, Label I)
	{
		T1 = A;
		T2 = B;
		T3 = C;
		B1 = D; 
		B2 = E;
		B3 = F;
		L1 = G;
		L2 = H;
		L3 = I; 
	}
}
